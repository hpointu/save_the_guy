pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
g = 0.05
gaz = 0.15
max_vh = 1.5
map_w = 46
map_h = 16
max_x = map_w * 8 - 16

function pos_eq(a, b)
 return flr(a.x) == flr(b.x) and flr(a.y) == flr(b.y)
end

function init_spawns()
	spawns = {}
	local s
	for x=0,map_w do
		for y=0,map_h do
		 s = mget(x, y)
			if(fget(s, 1)) add(spawns, {x=x*8, y=y*8})
		end
	end
end

function player_cells()
 local c = cell(pos)
	return {
		{x=c.x, y=c.y+1},
		{x=c.x+2, y=c.y+1},
	}
end

function player_rope_cells()
 local c = cell(pos)
 local back = {x=c.x, y=c.y+1}
 if(flipped) back = {x=c.x+2, y=c.y+1}
	return {
	 back,
		{x=c.x+1, y=c.y+1},
	}
end

function player_on_cell(cell)
	for c in all(player_cells()) do
		if pos_eq(cell, c) then return true end
	end
	return false
end


function busy(pos)
	for g in all(guys) do
		if(pos_eq(g, pos)) then return true end
	end
	return false
end

function spawn_guy()
	local i = flr(rnd(#spawns)) + 1
	local s = spawns[i]
 if(not busy(s))
 then
		local skin = flr(rnd(4))+1
		local skins = {9, 11, 25, 27}
		s.s = skins[skin]
 	add(guys, s)
		last_spawn = time()
	end
end

function init_game()
	pos = {x=40, y=10}
	game_over = false
	velocity = {x=0, y=0}
	cam = {x=0, y=0}
	guys = {}
	init_spawns()
	flipped = false
	score = 0
	for i=0,2 do spawn_guy() end
end

init_game()

function pickup_guy()
 local ropes = player_rope_cells()
	for g in all(guys) do
		local gc = cell(g)
		for rope in all(ropes) do
		 if pos_eq(rope, gc) then
		 	del(guys, g)
		 	score += fget(mget(gc.x, gc.y)) - 1
		 end
		end
	end
end

function update_camera()
	if (pos.x - cam.x > 90) then
		cam.x = pos.x - 90
	elseif (pos.x - cam.x < 30) then
	 cam.x = max(0, pos.x - 30)
	end
end

function cell(p)
	return {x=flr(p.x / 8) + 0,
	        y=flr(p.y / 8) + 0}
end

function player_flags()
	local cells = player_cells()
	local flags = 0
	for c in all(cells) do
	 flags = bor(flags, fget(mget(c.x, c.y)))
	end
 return flags
end

function _update()
	if(game_over) then
		if(btnp(❎))  init_game()
		return
	end
		
	if (btn(⬆️)) then velocity.y -= gaz end
	if (btn(⬅️))	then
	 velocity.x = max(velocity.x - gaz, -max_vh)
	 flipped = true
	end
	if (btn(➡️)) then
	 velocity.x = min(velocity.x + gaz, max_vh)
	 flipped = false
	end

	pos.x += velocity.x
	pos.y += velocity.y
	
	if pos.x < 0 then pos.x = 0; velocity.x = 0 end
	if pos.x > max_x then pos.x = max_x; velocity.x = 0 end
	if pos.y < 0 then pos.y = 0; velocity.y = 0 end
	velocity.y += g
	
	update_camera()
	pickup_guy()
	if(collides()) game_over = true	
	
	if(time() - last_spawn > 10) spawn_guy()
end

function draw_player()
	local s = 5
	if(flr(time()*8) % 2 == 1) s = 7
	spr(s, pos.x, pos.y, 2, 1, flipped)
end

function draw_guy(guy)
	local s = guy.s
	if(flr(time()*2) % 2 == 1) s += 1
	spr(s, guy.x, guy.y)
end

function _draw()
	cls(0)
	camera(cam.x, cam.y)
	palt(12, true)
	map(0, 0, 0, 0, map_w, 16)
	draw_player()
	foreach(guys, draw_guy)
 camera()
 print("score: "..score)
	if game_over then
		rectfill(20, 20, 108, 60, 1)
		print("game over", 40, 25, 7)
		print("press ❎ to restart", 30, 45, 7)
	end
end

function touches()
	local flags = player_flags()
	return band(flags, 1) == 1	
end

function collides()
	if (touches()) then return true end
	return false
end


